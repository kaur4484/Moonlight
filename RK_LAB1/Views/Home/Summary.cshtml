@model IEnumerable<RK_LAB1.Models.Student>
@{Layout = null;}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link rel="stylesheet" href="/lib/bootstrap/dist/css/bootstrap.css" />
    <title>ListResponses</title>
</head>
<body>
    <div class="panel-body">
        <h2>Here is the list of all info</h2>
        <table class="table table-smtable-striped table-bordered">
            <thead>
                <tr>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Selected Courses</th>
                    <th>Student Number</th>
                </tr>
            </thead>
            <tbody>
                @foreach (RK_LAB1.Models.Student r in Model)
                {
                <tr>
                    <td>@r.firstName</td>
                    <td>@r.lastName</td>
                    <td>@r.selectedCourses</td>
                    <td>@r.studentNo</td>
                </tr>
                }
            </tbody>
        </table>
    </div>
</body>
</html>